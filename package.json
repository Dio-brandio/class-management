{
  "name": "personal-base-setup",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel  src --out-dir dist --extensions \".ts,.js\" --copy-files",
    "build:dev": "cross-env NODE_ENV=dev esbuild src/index.ts --bundle --outfile=dist/index.js --minify --platform=node --loader:.html=text --packages=external",
    "build:prod": "cross-env NODE_ENV=prod esbuild src/index.ts --bundle --outfile=dist/index.js --minify --platform=node --loader:.html=text --packages=external",
    "build:stage": "cross-env NODE_ENV=stage esbuild src/index.ts --bundle --outfile=dist/index.js --minify --platform=node --loader:.html=text --packages=external",
    "start:dev": "npx cross-env NODE_ENV=dev node dist/index.js",
    "start:prod": "npx cross-env NODE_ENV=prod node dist/index.js",
    "start:stage": "npx cross-env NODE_ENV=stage node dist/index.js",
    "dev": " npx cross-env NODE_ENV=dev nodemon src/index.ts",
    "migrate:dev": "npx cross-env NODE_ENV=dev sequelize-cli db:migrate",
    "migrate:undo:dev": "npx cross-env NODE_ENV=dev npx sequelize-cli db:migrate:undo",
    "migrate:status:dev": "npx cross-env NODE_ENV=dev npx sequelize-cli db:migrate:status",
    "migrate:undo:all:dev": "npx cross-env NODE_ENV=dev npx sequelize-cli db:migrate:undo:all",
    "seed:dev": "npx cross-env NODE_ENV=dev sequelize-cli db:seed:all",
    "migrate:prod": "npx cross-env NODE_ENV=prod sequelize-cli db:migrate",
    "migrate:undo:prod": "npx cross-env NODE_ENV=prod npx sequelize-cli db:migrate:undo",
    "migrate:status:prod": "npx cross-env NODE_ENV=prod npx sequelize-cli db:migrate:status",
    "migrate:undo:all:prod": "npx cross-env NODE_ENV=prod npx sequelize-cli db:migrate:undo:all",
    "migrate:stage": "npx cross-env NODE_ENV=stage npx sequelize-cli db:migrate",
    "migrate:undo:stage": "npx cross-env NODE_ENV=stage npx sequelize-cli db:migrate:undo",
    "migrate:status:stage": "npx cross-env NODE_ENV=stage npx sequelize-cli db:migrate:status",
    "migrate:undo:all:stage": "npx cross-env NODE_ENV=stage npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jose": "^5.9.4",
    "json2csv": "^6.0.0-alpha.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^23.6.0",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "esbuild": "^0.24.0",
    "module-resolver": "^1.0.0",
    "nodemon": "^3.1.7",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}